<SFINCS_testbed>
    <config>
        <serverUrl>{PLACEHOLDER}</serverUrl>
        <localPaths>
            <casesDir>./data/cases</casesDir>
            <referenceDir>./data/references</referenceDir>
            <enginesDir>./data/engines</enginesDir>
            <resultsDir>./data/results</resultsDir>
        </localPaths>
    </config>

    <programs>
        <program name="SFINCS">
            <path>sfincs/SFINCS.exe</path>
        </program>
    </programs>

    <testCases>
        <testCase name="01_implementation_Drainage_culvert" ref="SFINCS">
            <path>01_implementation/Drainage_culvert</path>
            <checks>
 				<file name="sfincs_his.nc" type="NETCDF">
                    <parameters>
                        <parameter name="drainage_discharge" location="[1,2]" toleranceAbsolute="0.0001"/>						
                    </parameters>
                </file>
                <file name="sfincs_map.nc" type="NETCDF">
                </file>
			</checks>
            <maxRunTime>120.0</maxRunTime>
        </testCase>
        <testCase name="01_implementation_Drainage_pump" ref="SFINCS">
            <path>01_implementation/Drainage_pump</path>
            <checks>
 				<file name="sfincs_his.nc" type="NETCDF">
                    <parameters>
                        <parameter name="drainage_discharge" location="[1,2]" toleranceAbsolute="0.0001"/>						
                    </parameters>
                </file>
                <file name="sfincs_map.nc" type="NETCDF">
                </file>
			</checks>
            <maxRunTime>120.0</maxRunTime>
        </testCase>
    </testCases>
    <!--<latex_graphs>
        <latexGraph name="Charleston" his="true" his_loc="[1]" his_var="drainage_discharge">
            <data label="observations" />
            <map type="1D" var="" yloc="" time="" />
            <plot title
        </latexGraph>
    </latex_graphs>-->
</SFINCS_testbed>