# Build netcdf.inc file from netcdf2, netcdf3 and netcdf4 files

netcdf.inc: $(top_srcdir)/fortran/netcdf2.inc $(top_srcdir)/fortran/netcdf3.inc $(top_srcdir)/fortran/netcdf4.inc
	echo '!     NetCDF-3.' > netcdf.inc
	cat $(top_srcdir)/fortran/netcdf3.inc >> netcdf.inc
@USE_NETCDF4_TRUE@	echo >> netcdf.inc
@USE_NETCDF4_TRUE@	echo '!     NetCDF-4.' >> netcdf.inc
@USE_NETCDF4_TRUE@	cat $(top_srcdir)/fortran/netcdf4.inc >> netcdf.inc
@BUILD_V2_TRUE@	echo >> netcdf.inc
@BUILD_V2_TRUE@	echo '!     NetCDF-2.' >> netcdf.inc
@BUILD_V2_TRUE@	cat $(top_srcdir)/fortran/netcdf2.inc >> netcdf.inc
@USE_LOGGING_TRUE@	echo >> netcdf.inc
@USE_LOGGING_TRUE@	echo '!     This is to turn on netCDF internal logging.' >> netcdf.inc
@USE_LOGGING_TRUE@	echo '      integer nf_set_log_level' >> netcdf.inc
@USE_LOGGING_TRUE@	echo '      external nf_set_log_level' >> netcdf.inc

