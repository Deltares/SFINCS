LCHEADERS = $(am__append_7) $(am__append_9) $(am__append_11)
UCHEADERS = $(am__append_8) $(am__append_10) $(am__append_12)

# These are always used
@UPPER_CASE_MOD_TRUE@UCCOMMON = TYPESIZES.mod NETCDF.mod
@UPPER_CASE_MOD_FALSE@LCCOMMON = typesizes.mod netcdf.mod
@UPPER_CASE_MOD_FALSE@COMMONHEADERS = $(LCHEADERS) $(LCCOMMON)
@UPPER_CASE_MOD_TRUE@COMMONHEADERS = $(UCHEADERS) $(UCCOMMON)
nodist_include_HEADERS = $(COMMONHEADERS) netcdf.inc

